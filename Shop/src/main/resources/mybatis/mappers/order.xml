	<insert id="insertNewOrder" parameterType="orderVO">
		<![CDATA[
			insert into orders(order_num,
								total_Price,
								apply_num,
								order_status,
								pg,
								paid_at,
								member_id,
								member_name,
								email,
								phone,
								address,
								zipNo,
								delivery_request) values(
								#{order_num},
								#{totalPrice},
								#{apply_num},
								#{order_status},
								#{pg},
								#{paid_at},
								#{member_id},
								#{member_name},
								#{email},
								#{phone},
								#{address},  
								#{zipNo},
								#{delivery_request})
								   ]]>
		
	</insert>
	
	
	
</mapper>